<event name="MV_TRAVELER_OGT_BOSS_IRON_CHAOS">
	<text>From out of the tunnel comes a massive cruiser vessel, of Iron Watch design! This must be some rare exploration vessel, who's taken it upon themselves to travel the Multiverse!</text>
	<ship load="SHIP_OGT_BOSS_IRON_CHAOS" hostile="false" /> 
	<eventAlias jumpClear="false" name="OGT_BOSS_IRON_DROP">OGT_BOSS_IRON_DROP_CHAOS</eventAlias>
	<choice hidden="true">
		<text>Continue...</text>
		<event>
			<text>The ship turns to engage you.</text>
			<ship hostile="true" /> 
			<choice hidden="true">
				<text>Continue...</text>
				<event load="COMBAT_CHECK" />
			</choice>
		</event>
	</choice>
	<choice blue="false" req="BRIBE_CHAOS" lvl="1">
		<text id="renegade_tribute_scrap" />
		<event>
			<text id="renegade_tributed_chaos" />
			<playSound>gm_bribe</playSound>
			<variable name="gnome_bribe" op="set" val="1" />
			<item_modify>
				<item type="scrap" min="-45" max="-45" />
			</item_modify>
		</event>
	</choice>
	<choice hidden="true" blue="false" req="BRIBE_CHAOS" lvl="0" max_lvl="0">
