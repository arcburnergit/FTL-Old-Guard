		<text id="renegade_drop_fail" />
		<autoReward level="LOW">stuff</autoReward>
	</event>
	<event><loadEvent>OGT_BOSS_IRON_DROP</loadEvent></event>
</eventList>

<event name="OGT_BOSS_IRON_DEADCREW">
